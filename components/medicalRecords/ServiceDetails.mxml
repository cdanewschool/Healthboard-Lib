<?xml version="1.0" encoding="utf-8"?>
<s:NavigatorContent xmlns:fx="http://ns.adobe.com/mxml/2009" 
					xmlns:s="library://ns.adobe.com/flex/spark" 
					xmlns:mx="library://ns.adobe.com/flex/mx"
					label="{(serviceData.date != null ? serviceData.date : serviceData.inpdate) + ' - ' + serviceData.reason}" creationComplete="{populateNextSteps(); displayCorrespondingData();}">
	<fx:Script>
		<![CDATA[
			import mx.collections.ArrayCollection;
			import mx.controls.LinkButton;
			import mx.controls.Text;
			import mx.utils.ObjectProxy;
			[Bindable] public var serviceData:Object;
			public var myLinkButton:LinkButton;
			
			private function populateNextSteps():void {
				if(serviceData.nextSteps is ArrayCollection) {
					var myVGroup:VGroup;
					var myHGroup:HGroup;
					var myLblBullet:Text;
					var myTxtDescription:Text;
					//var myLinkButton:LinkButton;
					for(var i:uint = 0; i < serviceData.nextSteps.length; i++) {
						myVGroup = new VGroup();
						myHGroup = new HGroup();
						myHGroup.paddingLeft = 27;
						myHGroup.paddingBottom = -13;
						myHGroup.gap = 0;
						myLblBullet = new Text();
						myTxtDescription = new Text();
						myTxtDescription.width = 245;
						myLblBullet.htmlText = "â€¢";
						myTxtDescription.htmlText = serviceData.nextSteps[i].task;
						//if(serviceData.nextSteps[i].recommendation != '') myTxtDescription.htmlText += "<br>Recommendation: " + serviceData.nextSteps[i].recommendation;
						myLinkButton = new LinkButton();
						if(serviceData.nextSteps[i].recommendation != '') {
							myLinkButton.label = "Recommendation: " + serviceData.nextSteps[i].recommendation;
							myLinkButton.setStyle("paddingLeft",38);
							myLinkButton.setStyle("color","0xAEDEE4");
							myLinkButton.setStyle("textRollOverColor","0xAEDEE4");
							myLinkButton.setStyle("textSelectedColor","0xAEDEE4");
							myLinkButton.setStyle("skin",null);
							//myLinkButton.addEventListener(MouseEvent.ROLL_OVER,rolloverHandler);
							//myLinkButton.addEventListener(MouseEvent.ROLL_OUT,rolloutHandler);
							if(serviceData.nextSteps[i].recommendation == "Gentle Chair Yoga Class") myLinkButton.addEventListener(MouseEvent.CLICK,clickHandler);
						}
						myHGroup.addElement(myLblBullet);
						myHGroup.addElement(myTxtDescription);
						myVGroup.addElement(myHGroup);
						myVGroup.addElement(myLinkButton);
						vgNextSteps.addElement(myVGroup);
					}
				}
				else if(serviceData.nextSteps is ObjectProxy) {
					
				}
			}
			
			private function clickHandler(event:MouseEvent):void {
				parentApplication.handleRecommendation('Gentle Chair Yoga Class');
			}
			private function rolloverHandler(event:MouseEvent):void {
				myLinkButton.setStyle("styleName",'linkBtnUnderline');
				//myLinkButton.setStyle("rollOver",myLinkButton.styleName='linkBtnUnderline');		//(this also works)
			}
			private function rolloutHandler(event:MouseEvent):void {
				myLinkButton.setStyle("styleName",'linkBtn');
				//myLinkButton.setStyle("rollOut",myLinkButton.styleName='linkBtn');				//(this also works)
			}
			
			private function displayCorrespondingData():void {
				if(serviceData.name == "Labs") {
					vgLab.visible = vgLab.includeInLayout = vgDateCollectionOrder.visible = vgDateCollectionOrder.includeInLayout = true;
					vgVisit.visible = vgVisit.includeInLayout = vgRadiology.visible = vgRadiology.includeInLayout = hgDetailsInterpreter.visible = hgDetailsInterpreter.includeInLayout = vgDateTime.visible = vgDateTime.includeInLayout = hgClassification.visible = hgClassification.includeInLayout = false;
					lblDetailsTitle.text= "Lab Details";
					lblDetailsProvider.text = "Ordered by";
					lblReasonForVisit.text = "Lab Name";
				}
				else if(serviceData.name == "Radiology" || serviceData.name == "Procedures" || serviceData.name == "Surgeries") {
					vgRadiology.visible = vgRadiology.includeInLayout = hgDetailsInterpreter.visible = hgDetailsInterpreter.includeInLayout = vgDateTime.visible = vgDateTime.includeInLayout = hgClassification.visible = hgClassification.includeInLayout = true;
					vgLab.visible = vgLab.includeInLayout = vgVisit.visible = vgVisit.includeInLayout = vgDateCollectionOrder.visible = vgDateCollectionOrder.includeInLayout = false;
					lblDetailsTitle.text= "Procedure Details";
					lblDetailsProvider.text = "Ordered by";
					lblReasonForVisit.text = "Reason for Visit";
					if(serviceData.reason == "Nasal Procedure") {
						hgImagesNasal.visible = hgImagesNasal.includeInLayout = containerNasal.visible = containerNasal.includeInLayout = true;
						hgImagesColonoscopy.visible = hgImagesColonoscopy.includeInLayout = containerColonoscopy.visible = containerColonoscopy.includeInLayout = false;
					}
					else {
						hgImagesNasal.visible = hgImagesNasal.includeInLayout = containerNasal.visible = containerNasal.includeInLayout = false;
						hgImagesColonoscopy.visible = hgImagesColonoscopy.includeInLayout = containerColonoscopy.visible = containerColonoscopy.includeInLayout = true;
					}
				}
				else {
					vgVisit.visible = vgVisit.includeInLayout = vgDateTime.visible = vgDateTime.includeInLayout = hgClassification.visible = hgClassification.includeInLayout = true;
					vgLab.visible = vgLab.includeInLayout = vgRadiology.visible = vgRadiology.includeInLayout = hgDetailsInterpreter.visible = hgDetailsInterpreter.includeInLayout = vgDateCollectionOrder.visible = vgDateCollectionOrder.includeInLayout = false;
					lblDetailsTitle.text= "Service Details";
					lblDetailsProvider.text = "Provider";
					lblReasonForVisit.text = "Reason for Visit";
				}
			}
			
			private function truncateComments(item:Object, column:DataGridColumn):String {
				return item.Comments.length < 43 ? item.Comments : item.Comments.substr(0,43) + "...";
			}
		]]>
	</fx:Script>
